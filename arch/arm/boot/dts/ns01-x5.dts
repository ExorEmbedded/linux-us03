/dts-v1/;

#include <dt-bindings/input/input.h>
#include "imx6ul.dtsi"

/ {
    model = "Exor NanoSom01 i.MX6 UltraLite X5 Handheld Board";
    compatible = "exor,ns01-ek435", "fsl,imx6ul";

    aliases {
        i2c3 = &i2c1;
        i2c1 = &i2c2;
        i2c2 = &i2c3;
        i2c0 = &i2c4;
        serial0 = &uart1;
        serial1 = &uart6;
        serial2 = &uart5;
        serial3 = &uart4;
        serial4 = &uart3;
        serial5 = &uart2;
        serial6 = &uart7;
        serial7 = &uart8;
		watchdog0 = &wdog1;
    };

    chosen {
        stdout-path = &uart1;
    };

    memory {
        reg = <0x80000000 0x20000000>;
    };

    regulators {
        compatible = "simple-bus";
        #address-cells = <1>;
        #size-cells = <0>;

        dummy_reg: regulator@0 {
            compatible = "regulator-fixed";
            reg = <0>;
            regulator-name = "dummy-supply";
            regulator-boot-on;
        };

        vdd_reg: regulator@1 {
            compatible = "regulator-fixed";
            reg = <1>;
            regulator-name = "vdd-supply";
            gpio = <&gpio1 24 0>;
            enable-active-high;
            regulator-boot-on;
            regulator-always-on;
        };

        wlan_en_reg: fixedregulator@2 {
            compatible = "regulator-fixed";
            reg = <2>;
            regulator-name = "wlan-en-regulator";
            regulator-min-microvolt = <3300000>;
            regulator-max-microvolt = <3300000>;
            /* WLAN_EN GPIO for this board */
            gpio = <&gpio1 9 0>;
            /* WLAN card specific delay */
            startup-delay-us = <70000>;
            enable-active-high;
        };

		reg_adc_vref_3v3: regulator@2 {
			compatible = "regulator-fixed";
			regulator-name = "vref-3v3";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
		};

    };

    leds {
        compatible = "gpio-leds";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_leds>;

        led@1 {
            label = "ns01:dl:usr0";
            gpios = <&tca6408 7 1>;
            default-state = "on";
        };

        led@2 {
            label = "ns01:fault:usr0";
            gpios = <&tca6408 6 0>;
            default-state = "off";
        };

        led@3 {
            label = "x5:front:green";
            gpios = <&gpio3 27 0>;
            default-state = "on";
        };

        led@4 {
            label = "x5:front:red";
            gpios = <&gpio3 28 0>;
            default-state = "off";
        };
    };

    pwm_bl00: backlight {
        compatible = "pwm-backlight";
        pwms = <&pwm4 0 50000>;
        brightness-levels = <0 4 8 12 16 20 24 28 32 36 40 44 48 52 56 60 64 68 72 76 80 84 88 92 96 100>;
        default-brightness-level = <0>;
        enable-gpios = <&gpio1 25 0>;
        status = "okay";
    };

	beeper {
		/* Piezoelectric buzzer */
		compatible = "gpio-beeper";
		pinctrl-0 = <&pinctrl_beeper>;
		pinctrl-names = "default";
		gpios = <&gpio4 16 GPIO_ACTIVE_HIGH>;
	};

	matrix_keypad {
		compatible = "gpio-matrix-keypad";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_keypad>;
		debounce-delay-ms = <10>;
		col-scan-delay-ms = <10>;
		gpio-activelow;
		gpio-colactivehigh;
		engpio = <&gpio1 18 0>;
		shutdown-code=<0x00BB00BD>;	/* KEY_F17 (00BB) + KEY_F19 (00BD) */
		shutdown-count=<25>;		/* 25 * 200mS = 5 secs */
		status = "okay";

		/* Output */
		col-gpios = <
			&gpio2 8  0
			&gpio2 9  0
			&gpio2 10 0
			&gpio2 11 0
			&gpio2 12 0
		>;

		/* Input */
		row-gpios = <
			&gpio3 21 0
			&gpio3 22 0
			&gpio3 23 0
			&gpio3 24 0
		>;

		/* row << 24 | column << 16 | key-code */
		linux,keymap = <
			0x0000003B	/* KEY_F1	(0)	*/
			0x0100003C	/* KEY_F2	(5)	*/
			0x0200003D	/* KEY_F3	(10)	*/
			0x0300003E	/* KEY_F4	(15)	*/

			0x0001003F	/* KEY_F5	(1)	*/
			0x01010040	/* KEY_F6	(6)	*/
			0x02010041	/* KEY_F7	(11)	*/
			0x03010042	/* KEY_F8	(16)	*/

			0x00020043	/* KEY_F9	(2)	*/
			0x01020044	/* KEY_F10	(7)	*/
			0x02020057	/* KEY_F11	(12)	*/
			0x03020058	/* KEY_F12	(17)	*/

			0x000300B7	/* KEY_F13	(3)	*/
			0x010300B8	/* KEY_F14	(8)	*/
			0x020300B9	/* KEY_F15	(13)	*/
			0x030300BA	/* KEY_F16	(18)	*/

			0x000400BB	/* KEY_F17	(4)	*/
			0x010400BC	/* KEY_F18	(9)	*/
			0x020400BD	/* KEY_F19	(14)	*/
			0x03040000  /* KEY_RESERVED		*/
		>;
	};

    working_hours {
        compatible = "working_hours";
        eeprom = <&seeprom0>;
        rtcnvram = <&m41t83>;
        status = "okay";
    };

    rot_enc_right {
        compatible = "dual-rotary-encoder";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_encoderpad>;
        gpios = <&gpio4 10 0>, <&gpio4 12 0>;
        linux,axis = <0x08>; /* REL_WHEEL / ABS_WHEEL */
        rotary-encoder,encoding = "gray";
        rotary-encoder,steps = <32768>;
        rotary-encoder,rollover;
    };

    gpio-poweroff {
        compatible = "gpio-poweroff";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_poweroffpad>;
        gpios = <&gpio4 14 0>;
    };
};

&clks {
    assigned-clocks = <&clks IMX6UL_CLK_PLL4_AUDIO_DIV>;
    assigned-clock-rates = <786432000>;
};

&cpu0 {
    arm-supply = <&reg_arm>;
    soc-supply = <&reg_soc>;
};

&gpc {
    fsl,cpu_pupscr_sw2iso = <0x2>;
    fsl,cpu_pupscr_sw = <0x1>;
    fsl,cpu_pdnscr_iso2sw = <0x1>;
    fsl,cpu_pdnscr_iso = <0x1>;
    fsl,wdog-reset = <1>; /* watchdog select of reset source */
    fsl,ldo-bypass = <1>; /* use ldo-bypass, u-boot will check it and configure */
};



&uart1 {
    pinctrl-names = "default";
    status = "okay";
	/delete-property/dmas;
	/delete-property/dma-names;	
};

&uart4 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_uart4>;
    fsl,uart-has-rtscts;
    status = "okay";
};

&uart5 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_uart5>;
    fsl,uart-has-rtscts;
    status = "okay";
};

&usbotg1 {
    dr_mode = "host";
    status = "okay";
};

&usbotg2 {
    dr_mode = "host";
    status = "okay";
};

&usdhc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc1>;
	bus-width = <4>;
	vmmc-supply = <&wlan_en_reg>;
	non-removable;		/* non-removable is not a variable, the fact it is */
						/* listed is all that is used by driver  */
	cap-power-off-card;
	#address-cells = <1>;
	#size-cells = <0>;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	max-frequency = <25000000>;
	status = "okay";

	wlcore: wlcore@0 {
		compatible = "ti,wl1837";
		reg = <2>;
		interrupt-parent = <&gpio1>;
		interrupts = <27 IRQ_TYPE_EDGE_FALLING>;
	};
};

&usdhc2 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_usdhc2>;
    no-1-8-v;
    keep-power-in-suspend;
    enable-sdio-wakeup;
    status = "okay";
};

&i2c4 {
    clock-frequency = <100000>;
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_i2c4>;
    status = "okay";

    pmic: pfuze3000@08 {
        compatible = "fsl,pfuze3000";
        reg = <0x08>;

        regulators {
            sw1a_reg: sw1a {
                    regulator-min-microvolt = <700000>;
                    regulator-max-microvolt = <3300000>;
                    regulator-boot-on;
                    regulator-always-on;
                    regulator-ramp-delay = <6250>;
            };

            /* use sw1c_reg to align with pfuze100/pfuze200 */
            sw1c_reg: sw1b {
                regulator-min-microvolt = <700000>;
                regulator-max-microvolt = <1475000>;
                regulator-boot-on;
                regulator-always-on;
                regulator-ramp-delay = <6250>;
            };

            sw2_reg: sw2 {
                regulator-min-microvolt = <2500000>;
                regulator-max-microvolt = <3300000>;
                regulator-boot-on;
                regulator-always-on;
            };

            sw3a_reg: sw3 {
                regulator-min-microvolt = <900000>;
                regulator-max-microvolt = <1650000>;
                regulator-boot-on;
                regulator-always-on;
            };

            swbst_reg: swbst {
                regulator-min-microvolt = <5000000>;
                regulator-max-microvolt = <5150000>;
                regulator-boot-on;
                regulator-always-on;
            };

            snvs_reg: vsnvs {
                regulator-min-microvolt = <1000000>;
                regulator-max-microvolt = <3000000>;
                regulator-boot-on;
                regulator-always-on;
            };

            vref_reg: vrefddr {
                regulator-boot-on;
                regulator-always-on;
            };

            vgen1_reg: vldo1 {
                regulator-min-microvolt = <1800000>;
                regulator-max-microvolt = <3300000>;
                regulator-always-on;
            };

            vgen2_reg: vldo2 {
                regulator-min-microvolt = <800000>;
                regulator-max-microvolt = <1550000>;
                regulator-always-on;
            };

            vgen3_reg: vccsd {
                regulator-min-microvolt = <2850000>;
                regulator-max-microvolt = <3300000>;
                regulator-always-on;
            };

            vgen4_reg: v33 {
                regulator-min-microvolt = <2850000>;
                regulator-max-microvolt = <3300000>;
                regulator-always-on;
            };

            vgen5_reg: vldo3 {
                regulator-min-microvolt = <1800000>;
                regulator-max-microvolt = <3300000>;
                regulator-always-on;
            };

            vgen6_reg: vldo4 {
                regulator-min-microvolt = <1800000>;
                regulator-max-microvolt = <3300000>;
                regulator-always-on;
            };
        };
    };

    seeprom0:seeprom0@54 {
        compatible = "atmel,24c02";
        reg = <0x54>;
    };

    seeprom1:seeprom1@50 {
        compatible = "atmel,24c02";
        reg = <0x50>;
    };

    seeprom2:seeprom2@51 {
        compatible = "atmel,24c02";
        reg = <0x51>;
    };

    m41t83:m41t83@68 {
        compatible = "st,m41t83";
        reg = <0x68>;
    };

    tca6408: tca6408@21 {
        compatible = "ti,tca6408";
        interrupt-parent = <&gpio1>;
        interrupts = <3 0x8>;
        gpio-controller;
        #gpio-cells = <2>;
        interrupt-controller;
        #interrupt-cells = <2>;
        reg = <0x21>;
        restart-if-fails;
    };

    lis331dlh@18 {
        /* Accelerometer */
        compatible = "st,lis331dlh-accel";
        reg = <0x18>;
    };

    tsc2004@4b {
        compatible = "ti,tsc2004";
        reg = <0x4B>;
        vio-supply = <&dummy_reg>;

        //MX6UL_PAD_UART2_CTS_B__GPIO1_IO22
        //reset-gpios = <&gpio4 8 GPIO_ACTIVE_HIGH>;
        interrupts-extended = <&gpio1 22 IRQ_TYPE_EDGE_RISING>;

        touchscreen-fuzz-x = <4>;
        touchscreen-fuzz-y = <7>;
        touchscreen-fuzz-pressure = <2>;
        touchscreen-size-x = <4096>;
        touchscreen-size-y = <4096>;
        touchscreen-max-pressure = <2048>;

        ti,x-plate-ohms = <280>;
        ti,esd-recovery-timeout-ms = <8000>;
    };

    lm75@4c {
        compatible = "national,lm75";
        reg = <0x4C>;
        status = "okay";
    };

    ads1000@48 {
        compatible = "exor,ads1000";
        reg = <0x48>;
        mux-gpio = <&gpio4 13 0>;
        status = "okay";
    };

	pn547: pn547@28 {
		compatible = "nxp,pn547";
		reg = <0x28>;
		clock-frequency = <400000>;
		interrupt-parent = <&gpio1>;
		interrupt-gpios = <&gpio1 23 0>;
		enable-gpios = <&gpio4 15 0>;
	};
/*
	sx8654@49 {
		compatible = "semtech,sx8654";
		reg = <0x49>;
		interrupt-parent = <&gpio1>;
		interrupts = <22 IRQ_TYPE_EDGE_FALLING>;
	};
*/
};

&pwm4 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_pwm4>;
    clocks = <&clks IMX6UL_CLK_PWM4>,
     <&clks IMX6UL_CLK_PWM4>;
    status = "okay";
};

&fec1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet1>;
	phy-mode = "rmii";
	phy-handle = <&ethphy1>;
	phy-reset-gpios = <&gpio4 8 GPIO_ACTIVE_LOW>;
	phy-reset-duration = <30>;
	status = "disabled";    //no eth on Standard X5

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		ethphy1: ethernet-phy@1 {
			reg = <1>;
		};
	};
};

&ecspi1 {
	fsl,spi-num-chipselects = <1>;
	cs-gpios = <&gpio4 26 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1>;
	status = "okay";
	/delete-property/dmas;
	/delete-property/dma-names;	

    fram: at25@2 {
        /* CS0 FRAM */
	    compatible = "atmel,at25", "st,m95256";
	    reg = <0>;
	    spi-max-frequency = <20000000>;
	    pagesize = <64>;
	    size = <65536>;
	    address-width = <16>;
	};
};


&snvs_pwrkey {
    status = "disabled";
};

&lcdif {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_lcdif>;
    display = <&lcdout0>;
    status = "okay";

    lcdout0: lcdout {
        bits-per-pixel = <16>;
        bus-width = <16>;

        display-timings {
            native-mode = <&timing0>;
            timing0: timing0 {
            clock-frequency = <9200000>;
            hactive = <480>;
            vactive = <272>;
            hfront-porch = <8>;
            hback-porch = <4>;
            hsync-len = <41>;
            vback-porch = <2>;
            vfront-porch = <4>;
            vsync-len = <10>;

            hsync-active = <0>;
            vsync-active = <0>;
            de-active = <1>;
            pixelclk-active = <0>;
            };
        };
    };
};

&adc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_adc1>;
	vref-supply = <&reg_adc_vref_3v3>;
	num-channels = <9>;
	status = "okay";
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1>;
	imx6ul-ns01 {
		pinctrl_usdhc1: usdhc1grp {
			fsl,pins = <
				MX6UL_PAD_SD1_CMD__USDHC1_CMD     0x170F9
				MX6UL_PAD_SD1_CLK__USDHC1_CLK     0x100F9
				MX6UL_PAD_SD1_DATA0__USDHC1_DATA0 0x170F9
				MX6UL_PAD_SD1_DATA1__USDHC1_DATA1 0x170F9
				MX6UL_PAD_SD1_DATA2__USDHC1_DATA2 0x170F9
				MX6UL_PAD_SD1_DATA3__USDHC1_DATA3 0x170F9
				MX6UL_PAD_UART1_RTS_B__GPIO1_IO19 0x170F9 /* SD1 CD */
				MX6UL_PAD_UART1_CTS_B__GPIO1_IO18 0x170F9 /* OFF DIS */
				MX6UL_PAD_UART3_RTS_B__GPIO1_IO27 0x170F9 /* WIFI IRQ */
				MX6UL_PAD_UART2_RTS_B__GPIO1_IO23 0x170F9 /* NFC IRQ */
				MX6UL_PAD_NAND_CLE__GPIO4_IO15    0x170F9 /* NFC Enable */
			>;
		};

		pinctrl_usdhc2: usdhc2grp {
			fsl,pins = <
				MX6UL_PAD_NAND_RE_B__USDHC2_CLK             0x17059
				MX6UL_PAD_NAND_WE_B__USDHC2_CMD             0x17059
				MX6UL_PAD_NAND_DATA00__USDHC2_DATA0         0x17059
				MX6UL_PAD_NAND_DATA01__USDHC2_DATA1         0x17059
				MX6UL_PAD_NAND_DATA02__USDHC2_DATA2         0x17059
				MX6UL_PAD_NAND_DATA03__USDHC2_DATA3         0x17059
			>;
		};

		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				MX6UL_PAD_UART3_TX_DATA__GPIO1_IO24         0x17059 /* EN_VDD */
				MX6UL_PAD_UART3_RX_DATA__GPIO1_IO25         0x17059 /* EN_BL */

				MX6UL_PAD_NAND_DATA07__GPIO4_IO09           0x17059
				MX6UL_PAD_GPIO1_IO09__GPIO1_IO09            0x17059
				MX6UL_PAD_ENET2_RX_ER__GPIO2_IO15           0x17059
			>;
		};

		pinctrl_i2c4: i2c4grp {
			fsl,pins = <
				MX6UL_PAD_UART2_TX_DATA__I2C4_SCL           0x4001b8b0
				MX6UL_PAD_UART2_RX_DATA__I2C4_SDA           0x4001b8b0
				MX6UL_PAD_GPIO1_IO03__GPIO1_IO03            0x17059 /* IO EXP PCA6408 IRQ pin */
				MX6UL_PAD_UART2_CTS_B__GPIO1_IO22           0x17059 /* TOUCH CONTR TSC2004 IRQ pin */
				MX6UL_PAD_NAND_CE0_B__GPIO4_IO13            0x17059 /* ADC ADS1000 MUX pin */
			>;
		};

		pinctrl_lcdif: lcdifdatgrp {
			fsl,pins = <
				MX6UL_PAD_LCD_CLK__LCDIF_CLK                0x00000028
				MX6UL_PAD_LCD_ENABLE__LCDIF_ENABLE          0x00000028
				MX6UL_PAD_LCD_HSYNC__LCDIF_HSYNC            0x00000028
				MX6UL_PAD_LCD_VSYNC__LCDIF_VSYNC            0x00000028

				MX6UL_PAD_LCD_DATA00__LCDIF_DATA00          0x00000028
				MX6UL_PAD_LCD_DATA01__LCDIF_DATA01          0x00000028
				MX6UL_PAD_LCD_DATA02__LCDIF_DATA02          0x00000028
				MX6UL_PAD_LCD_DATA03__LCDIF_DATA03          0x00000028
				MX6UL_PAD_LCD_DATA04__LCDIF_DATA04          0x00000028
				MX6UL_PAD_LCD_DATA05__LCDIF_DATA05          0x00000028
				MX6UL_PAD_LCD_DATA06__LCDIF_DATA06          0x00000028
				MX6UL_PAD_LCD_DATA07__LCDIF_DATA07          0x00000028
				MX6UL_PAD_LCD_DATA08__LCDIF_DATA08          0x00000028
				MX6UL_PAD_LCD_DATA09__LCDIF_DATA09          0x00000028
				MX6UL_PAD_LCD_DATA10__LCDIF_DATA10          0x00000028
				MX6UL_PAD_LCD_DATA11__LCDIF_DATA11          0x00000028
				MX6UL_PAD_LCD_DATA12__LCDIF_DATA12          0x00000028
				MX6UL_PAD_LCD_DATA13__LCDIF_DATA13          0x00000028
				MX6UL_PAD_LCD_DATA14__LCDIF_DATA14          0x00000028
				MX6UL_PAD_LCD_DATA15__LCDIF_DATA15          0x00000028
			>;
		};

		pinctrl_pwm4: pwm4grp {
			fsl,pins = <
				MX6UL_PAD_NAND_WP_B__PWM4_OUT               0x110b0
			>;
		};

		pinctrl_beeper: beeper0grp {
			fsl,pins = <
				MX6UL_PAD_NAND_DQS__GPIO4_IO16                0x110b0
			>;
		};

		pinctrl_enet1: enet1grp {
			fsl,pins = <
				MX6UL_PAD_ENET1_RX_EN__ENET1_RX_EN          0x0000E0B1
				MX6UL_PAD_ENET1_RX_ER__ENET1_RX_ER          0x0000E0B1
				MX6UL_PAD_ENET1_RX_DATA0__ENET1_RDATA00     0x0000E0B1
				MX6UL_PAD_ENET1_RX_DATA1__ENET1_RDATA01     0x0000E0B1
				MX6UL_PAD_ENET1_TX_EN__ENET1_TX_EN          0x0000E0B1
				MX6UL_PAD_ENET1_TX_DATA0__ENET1_TDATA00     0x0000E0B1
				MX6UL_PAD_ENET1_TX_DATA1__ENET1_TDATA01     0x0000E0B1
				MX6UL_PAD_ENET1_TX_CLK__ENET1_REF_CLK1      0x4b01b0a8

				MX6UL_PAD_NAND_DATA06__GPIO4_IO08           0x0000E0B1
				MX6UL_PAD_GPIO1_IO07__ENET1_MDC             0x0001b0b0
				MX6UL_PAD_GPIO1_IO06__ENET1_MDIO            0x0001b0b0
			>;
		};

		pinctrl_leds: ledsgrp {
			fsl,pins = <
				MX6UL_PAD_LCD_DATA22__GPIO3_IO27            0x10b0
				MX6UL_PAD_LCD_DATA23__GPIO3_IO28            0x10b0
			>;
		};

		pinctrl_uart4: uart4grp {
			fsl,pins = <
				MX6UL_PAD_UART4_RX_DATA__UART4_DCE_RX       0x0001B0B1
				MX6UL_PAD_UART4_TX_DATA__UART4_DCE_TX       0x0001B0B1
			>;
		};

		pinctrl_uart5: uart5grp {
			fsl,pins = <
				MX6UL_PAD_UART5_RX_DATA__UART5_DCE_RX       0x0001B0B1
				MX6UL_PAD_UART5_TX_DATA__UART5_DCE_TX       0x0001B0B1
			>;
		};

		pinctrl_keypad: keypadgrp {
